---
export interface Props {
  title: string;
}

const {title} = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, minimum-scale=1.0" />
    <title>{title}</title>
    <slot name="head"></slot>
  </head>
  <body>
    <slot></slot>
  </body>
</html>